              }
            }
          ]
        }
      }
    ],
    "variables": [
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "hidden": false
          },
          "allowAllValue": false,
          "allowMultiple": false,
          "sort": "none",
          "plugin": {
            "kind": "PrometheusLabelValuesVariable",
            "spec": {
              "labelName": "job",
              "matchers": [
                "controller_runtime_reconcile_total{namespace=~\"$namespace\"}"
              ]
            }
          },
          "name": "job"
        }
      },
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "hidden": false
          },
          "allowAllValue": false,
          "allowMultiple": false,
          "sort": "none",
          "plugin": {
            "kind": "PrometheusLabelValuesVariable",
            "spec": {
              "labelName": "namespace",
              "matchers": [
                "controller_runtime_reconcile_total"
              ]
            }
          },
          "name": "namespace"
        }
      },
      {
        "kind": "ListVariable",
        "spec": {
          "display": {
            "name": "pod",
            "hidden": true
          },
          "allowAllValue": true,
          "allowMultiple": true,
          "sort": "none",
          "plugin": {
            "kind": "PrometheusLabelValuesVariable",
            "spec": {
              "labelName": "pod",
              "matchers": [
                "controller_runtime_reconcile_total{namespace=~\"$namespace\", job=~\"$job\"}"
              ]
            }
          },
          "name": "pod"
        }
      }
    ],
    "duration": "1h",
    "refreshInterval": "0s"
  }
}
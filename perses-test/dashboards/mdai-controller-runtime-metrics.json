            {
              "x": 14,
              "y": 0,
              "width": 10,
              "height": 8,
              "content": {
                "$ref": "#/spec/panels/0_2"
              }
            }
          ]
        }
      },
      {
        "kind": "Grid",
        "spec": {
          "display": {
            "title": "Work Queue Metrics",
            "collapse": {
              "open": true
            }
          },
          "items": [
            {
              "x": 0,
              "y": 0,
              "width": 3,
              "height": 8,
              "content": {
                "$ref": "#/spec/panels/1_0"
              }
            },
            {
              "x": 3,
              "y": 0,
              "width": 11,

  "kind": "Dashboard",
  "metadata": {
    "name": "mdai-use-dashboard-demo",
    "createdAt": "2025-07-11T17:30:01.038766257Z",
    "updatedAt": "2025-07-11T17:30:01.038766257Z",
    "version": 0,
    "project": "mdai"
